---
optimizations: [ConstantFolding,UnreachableCodeElimination,ConstantFolding,UnreachableCodeElimination]
expect:
    identical-to:
        is: |
          class UCE_If {
            public static void main(String[] args) {
              System.out.println(1);
            }
          }
        
stdout:
    is: "1\n"
---
class UCE_If {
  public static void main(String[] args) {
    boolean x = true || false;

    if (x) {
        System.out.println(1);
    } else {
        System.out.println(0);
    }
  }
}
