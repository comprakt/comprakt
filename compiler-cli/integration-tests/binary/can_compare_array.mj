class A {
    public static void main(String[] args) {
    	int[] a;

        if(a == null) {
            System.out.println(1);
        } else {
            System.out.println(0);
        }

        a = new int[2];
        a[0] = 1;
        a[1] = 2;

    	int[] b = new int[2];
        b[0] = 1;
        b[1] = 2;

        if(a == null) {
            System.out.println(1);
        } else {
            System.out.println(0);
        }
        
        if(a == b) {
            System.out.println(1);
        } else {
            System.out.println(0);
        }

        if(a == a) {
            System.out.println(1);
        } else {
            System.out.println(0);
        }
    }
}
